// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init
 
generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
 
model stats {
  id         String?    @id @default(auto()) @map("_id") @db.ObjectId
  Rk         Int?
  Player     String?
  Pos        String?
  Age        Int?
  Tm         String?
  MP         Int?
  TRB        Float?
  AST        Float?
  STL        Float?
  BLK        Float?
  PTS        Float?
}
